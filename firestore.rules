rules_version = '2';
service cloud.firestore {
    match /databases/{database}/documents {
        match /{document=**} {
            match /users/{userId} {
                allow read, write;
            }
            match /settings/{userId} {
                allow read, write;
            }
             match /tech {
                allow read;
            }
            match /tech/{techName} {
                allow read;
            }
            match /tech/{techName}/topic {
                allow read;
            }
            match /tech/{techName}/topic/{topicName} {
                allow write;
            }
        }
    }
}